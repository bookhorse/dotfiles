URxvt.locale: true
URxvt.utf8: true
URxvt*saveLines: 5000

!URxvt.font: xft:Liberation Mono:size=12
!xft:Symbola
!URxvt*font : xft:LiterationMonoPowerline Nerd Font:size=10
!URxvt*termName:screen-256color
!URxvt*font:xft:AnonymicePowerline Nerd Font:size=11
!URxvt*font: xft:DejaVu Sans Mono for Powerline:size=11
!URxvt.font: xft:LiterationMonoPowerline Nerd Font:style=Book:size:12:antialias=true:hinting=true
URxvt.font: xft:LiterationMono Nerd Font Mono:style=Book
!URxvt*letterSpace: -2
URxvt*termName:xterm-256color

URxvt.perl-ext-common:  default
!URxvt.tabbed.tabbar-fg: 8
!URxvt.tabbed.tabbar-bg: 0
!URxvt.tabbed.tab-fg:    2
!URxvt.tabbed.tab-bg:    0


