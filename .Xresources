!Xft.autohint: 0
!Xft.lcdfilter:  lcdnone
!Xft.hintstyle:  hintslight
!Xft.hinting: 1
!Xft.antialias: 1
!xft.dpi: 100

!Xft.rgba: rgb


!Xft.dpi: 96
! hintnone, hintslight, hintmedium, hintfull
!Xft.hintstyle: hintfull
!Xft.hinting: true
! lcdnone, lcddefault, lcdlight, lcdlegacy
